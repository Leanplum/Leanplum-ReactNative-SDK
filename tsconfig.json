{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "inlineSourceMap": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "declaration": true,
    "jsx": "react-native"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "typings"]
}
